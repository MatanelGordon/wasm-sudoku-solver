    "@vitejs/plugin-vue": "^5",
    "@vitejs/plugin-vue-jsx": "^4",
    "@vue/eslint-config-prettier": "^10",
    "@vue/eslint-config-typescript": "^14",
    "@vue/test-utils": "^2.3.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.32.0",
    "jsdom": "^25",
    "less": "^4.1.3",
